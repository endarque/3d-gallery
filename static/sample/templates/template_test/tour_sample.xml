              vlookatmin="0" vlookatmax="0" limitview="lookat"/>
        <image>
            <cube url="theme_white/pano_%s.jpg"/>
        </image>
        <addhs_settings video_close_click="false" youtube_close_click="false" keyboard_activation_of_the_editor="true"/>
        <cotrol usercontrol="off"/>
