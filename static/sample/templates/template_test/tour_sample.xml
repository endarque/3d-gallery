<krpano version="1.21" title="artbonbon VR Gallery">
ㄴ
    <include url="plugins/common.xml?nocache=%$random%"/>

    <action name="startup" autorun="onstart">
        <!-- krpano 가 시작되면 바로 scene을 로드 -->
        if(startscene === null || !scene[get(startscene)], copy(startscene,scene[0].name); );
        loadscene(get(startscene), null, MERGE);
    </action>

    <!-- 회전값 설정 -->
    <autorotate enabled="true" waittime="1" speed="3" horizon="0.0" tofov="360"/>

    <!-- *** add hotspt *** -->
	<editor_hs pin="off" />

